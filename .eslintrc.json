{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["react-app", "react-app/jest"],
    "rules": {
        "no-console": "warn",
        "import/prefer-default-export": "off",
        "react/jsx-no-useless-fragment": "warn",
        "import/order": [
            "warn",
            {
                "groups": [
                    "builtin",
                    "external",
                    "internal",
                    "parent",
                    "sibling",
                    "index",
                    "object",
                    "type"
                ],
                "newlines-between": "always-and-inside-groups"
            }
        ]
    }
}
